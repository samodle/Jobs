                            <Canvas x:Name="Dimension2Canvas" Height="60" Canvas.Left="3" Canvas.Top="90" Width="173" Background="#FF64F5FF" Cursor="Hand" MouseMove="CriteriaCanvasMouseMove" MouseLeave="CriteriaCanvasMouseLeave" MouseDown="CriteriaCanvasClicked">
                                <Label x:Name="Dimension2_Magnitude" Content="230819" Canvas.Top="-3" Height="67" Width="102" Foreground="#FF585858" FontSize="29.333" FontFamily="Global Sans Serif" Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" FontWeight="Bold" Canvas.Left="8"/>
