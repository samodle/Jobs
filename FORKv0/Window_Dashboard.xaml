                        <ComboBox x:Name="AddSecondaryAxis_ComboBox" Canvas.Left="41" Canvas.Top="25" Width="130" Foreground="#FF666666" FontFamily="Microsoft Sans Serif" Height="19" Padding="2" VerticalContentAlignment="Center" Background="White" Cursor="Hand" HorizontalContentAlignment="Center" BorderBrush="#FFE6E6E6" BorderThickness="0.2" SelectionChanged="SecondaryAxisSelected" Visibility="Hidden"/>
                    </Canvas>

