                            <Canvas x:Name="Dimension4Canvas" Height="60" Canvas.Left="3" Canvas.Top="230" Width="173" Background="#FFEBEBEB" Cursor="Hand" MouseMove="CriteriaCanvasMouseMove" MouseLeave="CriteriaCanvasMouseLeave" MouseDown="CriteriaCanvasClicked">
                                <Label x:Name="Dimension4_Magnitude" Content="23.5" Canvas.Top="-3" Height="67" Width="102" Foreground="#FF585858" FontSize="29.333" FontFamily="Global Sans Serif" Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" FontWeight="Bold" Canvas.Left="8"/>
                                <Label x:Name="Dimension4_Description" Content="Skill&#xD;&#xA;Index" Canvas.Left="109" Canvas.Top="12" Height="34" Width="56" Foreground="#FF585858" FontSize="11" FontFamily="Global Sans Serif" Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                            </Canvas>
                            <Canvas x:Name="Dimension5Canvas" Height="60" Canvas.Left="3" Canvas.Top="300" Width="173" Background="#FFEBEBEB" Cursor="Hand" MouseMove="CriteriaCanvasMouseMove" MouseLeave="CriteriaCanvasMouseLeave" MouseDown="CriteriaCanvasClicked">
                                <Label x:Name="Dimension5_Magnitude" Content="235" Canvas.Top="-3" Height="67" Width="102" Foreground="#FF585858" FontSize="29.333" FontFamily="Global Sans Serif" Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" FontWeight="Bold" Canvas.Left="8"/>
                                <Label x:Name="Dimension5_Description" Content="Actual&#xD;&#xA;Applications" Canvas.Left="109" Canvas.Top="12" Height="34" Width="56" Foreground="#FF585858" FontSize="11" FontFamily="Global Sans Serif" Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
