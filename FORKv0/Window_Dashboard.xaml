                        <Label x:Name="PriorityIndexLabel_HighLabel" Content="High"  Height="24" Width="26" Foreground="#FF666666" FontFamily="Microsoft Sans Serif" Padding="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Canvas.Left="140" Canvas.Top="51" FontSize="9.333" />
                    </Canvas>

                    <Canvas x:Name="LossCompass_MainChartsarea" Height="556" Width="1022" Canvas.Top="31" Canvas.Left="10" Background="White">
                        <Label x:Name="PreviousCardsLabel" Content="Previous cards:" Canvas.Top="-1.5" Foreground="#FF494949" FontSize="9.333" Height="13" Padding="0" Visibility="Hidden" />
                        <Image x:Name="LossCompass_CanvasPickerButton" Height="28" Canvas.Left="995" Width="27" Source="Images/CanvasPicker.png" MouseDown="PickCanvas" MouseMove="Generalmousemove" MouseLeave="Generalmouseleave" ToolTip="Capture Loss Compass Charts in CANVAS" Visibility="Hidden" />
                    </Canvas>
                    <Image x:Name="LossCompass_MainFilter_ON" Height="24" Canvas.Left="922" Canvas.Top="4" Width="26" Source="Images/FilterIconFLat.png" Cursor="Hand" MouseMove="Generalmousemove" MouseLeave="Generalmouseleave" MouseDown="LaunchFilter"/>
                    <Label x:Name="FunnelAnalysisLaunchIcon" Content="Funnel Analysis" Canvas.Left="945" Canvas.Top="6" Width="84" Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Foreground="#FF666666" Height="24" MouseDown="LaunchFilter" Cursor="Hand"/>

                    <!--LossBottomLineCanvas-->
                    <Expander x:Name="LossBottomLineExpander" Header="Loss Bottom Line" Width="918" Foreground="#FF666666" ExpandDirection="Left" VerticalContentAlignment="Center" Canvas.Top="539" Padding="10,1,8,1" Canvas.Left="1" Expanded="LossBottomLineExpanded" Collapsed="LossBottomLineCollapsed" Visibility="Hidden">
                        <Grid x:Name="LossbottomlineExpandergrid" Background="#FFE5E5E5">
                            <Label x:Name="prBar" Height="40" Width="789" Margin="0,0,-0.2,0" Background="#FFEEEEEE" Foreground="#FF666666" FontFamily="Microsoft Sans Serif"  Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Cursor="Hand" MouseDown="PlannedUnplannedBarClicked" HorizontalAlignment="Left" BorderBrush="#FFE0E0E0" BorderThickness="1"/>
                            <Label x:Name="UnplannedBar" Height="40" Width="355" Margin="0,0,0,0" Background="#FF32CDF0" Foreground="#FF666666" FontFamily="Microsoft Sans Serif" Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Cursor="Hand" MouseDown="PlannedUnplannedBarClicked" HorizontalAlignment="Left" BorderBrush="#FFE0E0E0" BorderThickness="1"/>
                            <Label x:Name="PlannedBar" Height="40" Width="355"  Margin="0,0,0,0" Background="#FFD9D9D9" Foreground="#FF666666" FontFamily="Microsoft Sans Serif"  Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Cursor="Hand" MouseDown="PlannedUnplannedBarClicked" HorizontalAlignment="Left" BorderBrush="#FFE0E0E0" BorderThickness="1"/>
                            <Label x:Name="RatelossBar" Height="40" Width="355" Margin="0,0,0,0" Background="#FF32CDF0" Foreground="#FF666666" FontFamily="Microsoft Sans Serif"  Padding="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Cursor="Hand" MouseDown="PlannedUnplannedBarClicked" HorizontalAlignment="Left" BorderBrush="#FFE0E0E0" BorderThickness="1"/>

                        </Grid>
                    </Expander>
                    <Separator Height="6" Canvas.Left="1040" Margin="0" Canvas.Top="505" Width="171" Background="#FFDADADA"/>


