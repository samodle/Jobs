                            <Image x:Name="KPI1LockOFF" Height="15" Canvas.Left="181" Canvas.Top="42" Width="11" Source="Images/Lockicongrayscale.png" Cursor="Hand" Visibility="Visible" MouseDown="LockClicked" MouseMove="LockMouseMove" MouseLeave="LockMouseLeave"/>

                            <Image x:Name="KPI2LockON" Height="14" Canvas.Left="181" Canvas.Top="113" Width="11" Source="Images/Lockicon.png" Cursor="Hand" Visibility="Visible" MouseDown="LockClicked" MouseMove="LockMouseMove" MouseLeave="LockMouseLeave"/>
                            <Image x:Name="KPI2LockOFF" Height="15" Canvas.Left="181" Canvas.Top="113" Width="11" Source="Images/Lockicongrayscale.png" Cursor="Hand" Visibility="Visible" MouseDown="LockClicked" MouseMove="LockMouseMove" MouseLeave="LockMouseLeave"/>

                            <Image x:Name="KPI3LockON" Height="14" Canvas.Left="181" Canvas.Top="183" Width="11" Source="Images/Lockicon.png" Cursor="Hand" Visibility="Visible" MouseDown="LockClicked" MouseMove="LockMouseMove" MouseLeave="LockMouseLeave"/>
                            <Image x:Name="KPI3LockOFF" Height="15" Canvas.Left="181" Canvas.Top="183" Width="11" Source="Images/Lockicongrayscale.png" Cursor="Hand" Visibility="Visible" MouseDown="LockClicked" MouseMove="LockMouseMove" MouseLeave="LockMouseLeave"/>

                            <Image x:Name="KPI4LockON" Height="14" Canvas.Left="181" Canvas.Top="252" Width="11" Source="Images/Lockicon.png" Cursor="Hand" Visibility="Visible" MouseDown="LockClicked" MouseMove="LockMouseMove" MouseLeave="LockMouseLeave"/>
                            <Image x:Name="KPI4LockOFF" Height="15" Canvas.Left="181" Canvas.Top="252" Width="11" Source="Images/Lockicongrayscale.png" Cursor="Hand" Visibility="Visible" MouseDown="LockClicked" MouseMove="LockMouseMove" MouseLeave="LockMouseLeave"/>

