                        <Rectangle x:Name="ColumnLegendRectangle" Fill="#FF32CDF0" Height="13" Canvas.Left="20" Canvas.Top="6" Width="21"/>
                        <Label x:Name="ColumnLegendLabel" Content="Percent of Active Posts" Height="22" Foreground="#FF767676" FontFamily="Microsoft Sans Serif" Padding="1" Width="134" VerticalContentAlignment="Center" Canvas.Left="45" Canvas.Top="2"/>
                        <Image x:Name="AddSecondaryAxis_Image" Height="16" Canvas.Left="17" Canvas.Top="26" Width="27" Source="Images/BluePlusCircle.png" Cursor="Hand" MouseDown="AddSecondaryAxisClicked" OpacityMask="Black" />
