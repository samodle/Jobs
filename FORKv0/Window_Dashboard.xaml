<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
    x:Class="Windows_Desktop.dashboardwindow"
        mc:Ignorable="d"
        Loaded="fork_onload"      
        DataContext="{Binding RelativeSource={RelativeSource Self}}"
        Title="Fork"  Height="706" Width="1250" ResizeMode="CanResizeWithGrip" MinHeight="451" MinWidth="800"       
        WindowStartupLocation="CenterScreen"   >
    <Window.Resources>
        <ResourceDictionary>
            <Style x:Key="TransparentTickStyle" TargetType="FrameworkElement">
                <Setter Property="Visibility" Value="Collapsed" />
            </Style>

            <Style x:Key="CustomPin" TargetType="Path">
                <Setter Property="Fill" Value="#FFA3A3A3" />
                <Setter Property="Width" Value="5" />
                <Setter Property="Height" Value="10" />
                <Setter Property="Stretch" Value="Fill" />
                <Setter Property="telerik:RadialScale.RotationMode" Value="SurroundOut" />
                <Setter Property="telerik:ScaleObject.Offset" Value="0.01*" />
                <Setter Property="telerik:ScaleObject.Location" Value="Outside" />
                <Setter Property="Canvas.ZIndex" Value="501" />
            </Style>

            <Style x:Key="CustomPinLabel" TargetType="TextBlock">
                <Setter Property="Width" Value="70" />
                <Setter Property="TextWrapping" Value="Wrap" />
                <Setter Property="telerik:ScaleObject.Location" Value="Outside" />
                <Setter Property="telerik:ScaleObject.Offset" Value="0.18*" />
                <Setter Property="FontFamily" Value="Segoe UI" />
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="TextAlignment" Value="Center"/>
                <Setter Property="FontSize" Value="10" />
            </Style>
            <Style x:Key="CustomPinLabel2" TargetType="TextBlock">
                <Setter Property="Width" Value="70" />
                <Setter Property="TextWrapping" Value="Wrap" />
                <Setter Property="telerik:ScaleObject.Location" Value="Outside" />
                <Setter Property="telerik:ScaleObject.Offset" Value="0.1*" />
                <Setter Property="FontFamily" Value="Segoe UI" />
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="TextAlignment" Value="Center"/>
                <Setter Property="FontSize" Value="10" />
            </Style>

            <Style x:Key="NetworkBallStyle" TargetType="Ellipse">
                <Setter Property="Fill" Value="AliceBlue" />
                <Setter Property="Stroke" Value="#4682b4" />
                <Setter Property="StrokeThickness" Value="4" />
                <Setter Property="Height" Value="40" />
                <Setter Property="Width" Value="40" />
            </Style>

            <Style TargetType="FrameworkElement" x:Key="VisibleAnimation">
                <Setter Property="Visibility" Value="Visible"/>
                <Setter Property="Opacity" Value="1"/>
                <Style.Triggers>
                    <Trigger Property="Visibility" Value="Visible">
                        <Trigger.EnterActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetProperty="Opacity"
                             From="0.0" To="1.0" Duration="0:0:0.2"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </Trigger.EnterActions>
                    </Trigger>
                </Style.Triggers>
            </Style>

            <DataTemplate x:Key="LabelEmpty">
                <Grid />
            </DataTemplate>

        </ResourceDictionary>
    </Window.Resources>

